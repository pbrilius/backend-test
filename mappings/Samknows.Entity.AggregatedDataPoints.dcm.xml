      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="downloadAvg" type="float" precision="2" scale="8" column="download_avg" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uploadMin" type="integer" column="upload_min" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uploadMedian" type="integer" column="upload_median" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uploadMax" type="integer" column="upload_max" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="uploadAvg" type="float" precision="2" scale="8" column="upload_avg" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="latencyMin" type="integer" column="latency_min" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="latencyMedian" type="integer" column="latency_median" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="latencyMax" type="integer" column="latency_max" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="latencyAvg" type="float" precision="2" scale="8" column="latency_avg" nullable="false">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="packetLossMin" type="float" column="packet_loss_min" precision="2" scale="6" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="packetLossMedian" type="float" column="packet_loss_median" precision="2" scale="6" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="packetLossMax" type="float" column="packet_loss_max" precision="2" scale="6" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
